<%= render "home/navbar" %>

<div class=" container">
<h2 class="my-4"> Search Result</h2>
	<ul>

		<% if @property.present? %>
		<h3 class="text-center"> Properties Found : <%= @property.all.count %></h3>
		<% @property.each do |property| %>
			<%= render :partial => "rent_properties/list", locals: {property: property} %>	
		<% end %>
		<% else %>
			<h3 class="text-center">  Property Not Found </h3>
		<% end %>
	</ul>
</div>

<%= render "home/footer" %>
